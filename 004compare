score = int(input("score: "))

if score < 60:
    print('grade: f')
elif score >= 60:
    print('geade: d')
elif score >= 70:
    print('grade: c')
elif 80 <= score:
    print('grade: b')
else:
    print('grade: a')
    
    


def main():
    x = int(input("what is x?"))
    if is_even(x):
        print('even')
    else:
        print('odd')

def is_even(n):
    if n % 2 == 0:
        return True
    else:
        return False
    #other ways to do same things
    #return True if n % 2 == 0 else False
    #return n % 2 == 0 # in this case you even don't need to write true or false

main()
